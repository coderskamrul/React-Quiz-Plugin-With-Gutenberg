{
  "name": "react-quiz-app",
  "version": "1.0.0",
  "private": true,
  "description": "WordPress plugin for creating and displaying quizzes using React",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production &&  wp-scripts build && wp i18n make-pot . languages/react-quiz-app.pot --exclude=src,build,node_modules",
    "format": "wp-scripts format",
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip",
    "start": "npx cross-env NODE_ENV=development webpack --config=webpack.config.js --watch",
    "dev": "npx cross-env NODE_ENV=development wp-scripts start"
  },
  "prettier": "@wordpress/prettier-config",
  "author": "Kamrul Hasan",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@svgr/webpack": "^8.1.0",
    "@woocommerce/dependency-extraction-webpack-plugin": "^3.0.0",
    "@wordpress/api-fetch": "^6.54.0",
    "@wordpress/eslint-plugin": "^22.0.0",
    "@wordpress/i18n": "^5.23.0",
    "@wordpress/prettier-config": "^4.16.0",
    "@wordpress/scripts": "^30.0.2",
    "babel-loader": "^9.2.1",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.37.4",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.9.0",
    "prettier": "npm:wp-prettier@^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "svgo": "^3.2.0",
    "svgo-loader": "^4.0.0",
    "typescript": "^5.7.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4"
  }
}
